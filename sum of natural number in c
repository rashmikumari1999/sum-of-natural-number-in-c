#include <stdio.h>

int main()
{
    int i,N,s=0;
    printf("/n enter a natural number to calculate their sum");
    scanf("%d",&N);
    for(i=0;i<=N;i++)
    s=s+i;
    printf("calculate the number of sum is%d",s);
}
